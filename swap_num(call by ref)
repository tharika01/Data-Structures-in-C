#include <stdio.h>

int swap(int *p,int *q)
{
    int temp,s;
    temp = *p;
    *p = *q;
    *q = temp;
    return s;
}
void main()
{
    int a,b,s;
    printf("Enter 2 number :");
    scanf("%d %d",&a,&b);
    printf("Before Swap :%d %d\n",a,b);
    s = swap(&a,&b);
    printf("After Swap :%d %d",a,b);
}
